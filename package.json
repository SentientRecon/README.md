{
  "name": "sentient-recon-agent",
  "version": "1.0.0",
  "description": "Advanced Sentient Recon Agent (SRA) platform for cybersecurity operations",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "jest",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "cybersecurity",
    "recon",
    "SOC",
    "threat-intelligence",
    "incident-response",
    "compliance"
  ],
  "author": "SRA Development Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0"
  },
  "dependencies": {}
}