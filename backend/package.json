{
  "name": "sra-backend",
  "version": "1.0.0",
  "description": "Sentient Recon Agent Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:ts\" \"npm run dev:python\"",
    "dev:ts": "tsx watch src/index.ts",
    "dev:python": "python -m uvicorn python_api.main:app --reload --port 8001",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:python": "python -m uvicorn python_api.main:app --host 0.0.0.0 --port 8001",
    "test": "jest",
    "lint": "eslint src --ext .ts,.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "setup": "npm install && npm run db:generate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.5-lts.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/crypto-js": "^4.2.1",
    "@types/compression": "^1.7.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "cybersecurity",
    "soc",
    "threat-intelligence",
    "incident-response",
    "compliance",
    "api"
  ],
  "author": "SRA Development Team",
  "license": "MIT"
}